{
  "name": "vex-vue",
  "version": "0.1.0",
  "private": true,
  "window": {
    "title": "Project VEX",
    "toolbar": false,
    "id": "vexMainWindow",
    "width": 640,
    "height": 480
  },
  "chromium-args": "--enable-direct-write --allow-file-access-from-files",
  "snapshot": "",
  "node-remote": "<all_urls>",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "main": "http://127.0.0.1:8080",
  "dependencies": {
    "bootstrap-vue": "^2.0.0-rc.11",
    "jquery": "^3.3.1",
    "vue": "^2.5.17",
    "vue-plyr": "^5.0.4",
    "vue-router": "^3.0.1",
    "vue-slick": "^1.1.15"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.1.1",
    "@vue/cli-plugin-eslint": "^3.1.5",
    "@vue/cli-service": "^3.1.4",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "node-sass": "^4.10.0",
    "vue-materialize": "^1.0.0",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}